<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModels="clr-namespace:Jaguar.Desktop.ViewModels"
        xmlns:v="using:Jaguar.Desktop.CustomViews.ManuBars"
        xmlns:views="clr-namespace:Jaguar.Desktop.Views"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Jaguar.Desktop.Views.MainWindow"
        x:DataType="viewModels:MainWindowViewModel"
        Title="Jaguar.Desktop">
    <Design.DataContext>
        <viewModels:MainWindowViewModel/>
    </Design.DataContext>
    
   <DockPanel LastChildFill="True">
       <v:TopMenuBarView DockPanel.Dock="Top"/>
       <v:LeftMenuBarView DockPanel.Dock="Left"/>
       <v:RightMenuBarView DockPanel.Dock="Right"/>
       <v:BottomMenuBarView DockPanel.Dock="Bottom"/>
       
       <Border Padding="5" CornerRadius="10">
           <!-- <ContentControl Content="{Binding Content}"/> -->
           <views:WorkflowView/>
       </Border>
   </DockPanel>
</Window>
